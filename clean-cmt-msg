/^Subject: /,/^---$/ {
	# Strip android-specific tags: Change-Id: Depends-Change-Id: Orig-Change-Id:
	/^Change-Id: I[0-9a-f]\{40\}$/d
	/^Depends-Change-Id: I[0-9a-f]\{40\}$/d
	/^Orig-Change-Id: I[0-9a-f]\{40\}$/d
}
